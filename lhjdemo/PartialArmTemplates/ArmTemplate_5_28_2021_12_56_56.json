{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "lhjdemo"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/api_to_sql')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "ETL_API_TO_SQL",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"source": {
								"type": "RestSource",
								"httpRequestTimeout": "00:01:40",
								"requestInterval": "00.00:00:00.010",
								"requestMethod": "GET"
							},
							"sink": {
								"type": "AzureSqlSink"
							},
							"enableStaging": false,
							"translator": {
								"type": "TabularTranslator",
								"mappings": [
									{
										"source": {
											"path": "$['year']"
										},
										"sink": {
											"name": "Year",
											"type": "Int32"
										}
									},
									{
										"source": {
											"path": "$['cause_of_death']"
										},
										"sink": {
											"name": "cause_of_death",
											"type": "String"
										}
									},
									{
										"source": {
											"path": "$['number_of_deaths']"
										},
										"sink": {
											"name": "number_of_death",
											"type": "Int32"
										}
									},
									{
										"source": {
											"path": "$['age_adjusted_death_rate_per_100_000_population']"
										},
										"sink": {
											"name": "age_adjusted_death_rate_per_100_000_population",
											"type": "Decimal"
										}
									},
									{
										"source": {
											"path": "$['lower_95_confidence_limit']"
										},
										"sink": {
											"name": "lower_95_confidence_limit",
											"type": "Decimal"
										}
									},
									{
										"source": {
											"path": "$['upper_95_confidence_limit']"
										},
										"sink": {
											"name": "upper_95_confidence_limit",
											"type": "Decimal"
										}
									}
								]
							}
						},
						"inputs": [
							{
								"referenceName": "API_to_Sql",
								"type": "DatasetReference"
							}
						],
						"outputs": [
							{
								"referenceName": "njo_gov",
								"type": "DatasetReference"
							}
						]
					}
				],
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/API_to_Sql')]",
				"[concat(variables('factoryId'), '/datasets/njo_gov')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/API_to_Sql')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "RestService1",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "RestResource",
				"schema": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/RestService1')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/RestService1')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "RestService",
				"typeProperties": {
					"url": "https://data.nj.gov/resource/pruu-wapw.json",
					"enableServerCertificateValidation": true,
					"authenticationType": "Anonymous"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/njo_gov')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "Linked_Service_to_SQL",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "AzureSqlTable",
				"schema": [
					{
						"name": "Year",
						"type": "int",
						"precision": 10
					},
					{
						"name": "cause_of_death",
						"type": "varchar"
					},
					{
						"name": "number_of_death",
						"type": "int",
						"precision": 10
					},
					{
						"name": "age_adjusted_death_rate_per_100_000_population",
						"type": "decimal",
						"precision": 18,
						"scale": 2
					},
					{
						"name": "lower_95_confidence_limit",
						"type": "decimal",
						"precision": 18,
						"scale": 2
					},
					{
						"name": "upper_95_confidence_limit",
						"type": "decimal",
						"precision": 18,
						"scale": 2
					}
				],
				"typeProperties": {
					"schema": "dbo",
					"table": "njo_gov_data"
				}
			},
			"dependsOn": []
		}
	]
}